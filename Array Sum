#include <iostream>
using namespace std;

int main() {

  int n;
  cin >> n;

  int input[1000000];

  // Array Input
  for (int i = 0; i < n; i++) {
    cin >> input[i];
  }

  // Array Sum
  int sum = 0;
  for (int i = 0; i < n; i++) {
    sum += input[i];
  }
  cout << sum << endl;

  return 0;
}
