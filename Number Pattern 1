#include<iostream>
using namespace std;


int main(){

     int n;
     cin >> n;
    
    int i = 1;
    int val = 1;
    while (i<=n)
    {
        int temp;
        int j = 1;
        while (j<= i)
        {
            cout << val;
            j++;
            val++;
            
            if (j==2)
            {
                temp = val;
            }
        }
        val = temp;
        cout << endl;
        i++;
    }
    
}

