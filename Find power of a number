#include<iostream>
using namespace std;

int main() {
	// Write your code here
	int x , n;
    cin >> x >>n;
    
    int ans = 1;
    if(n ==0)
    {
        ans = 1;
    }
    else if (x ==0)
    {
        ans = 0;
    }
    else
    {
        int i = 1;
        while (i<= n)
        {
            ans*= x;
            i++;
        }
    }
    cout << ans;
    return 0;
}
